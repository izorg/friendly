{
  "name": "friendly",
  "scripts": {
    "build": "rm -rf build && npm run build:webpack && npm run build:babel && npm run build:graphql",
    "build:babel": "NODE_ENV=production BROWSERSLIST_ENV=node babel src/server --extensions \".ts\" --ignore src/server/middlewares/render --out-dir build/server --source-maps inline",
    "build:graphql": "cp src/server/middlewares/api/extend.graphql build/server/middlewares/api/extend.graphql",
    "build:webpack": "NODE_ENV=production webpack",
    "predev": "concurrently npm:predev:*",
    "predev:graphql": "NODE_ENV=development graphql-mesh dump-schema --output src/server/middlewares/api/schema.generated.graphql && prettier --write src/server/middlewares/api/schema.generated.graphql",
    "predev:typescript": "NODE_ENV=development graphql-mesh typescript --output src/server/middlewares/api/types.generated.ts && prettier --write src/server/middlewares/api/types.generated.ts",
    "dev": "concurrently npm:dev:*",
    "dev:codegen": "graphql-codegen --watch",
    "dev:server": "NODE_ENV=development BROWSERSLIST_ENV=node nodemon --exec \"babel-node -x .ts\" --ext graphql,ts --watch src/server src/server/index.ts",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "generate": "graphql-codegen",
    "mock": "concurrently npm:mock:*",
    "mock:leaflet": "prism mock -p 3001 src/server/middlewares/api/contracts/leaflet.yaml",
    "mock:pim": "prism mock -p 3002 src/server/middlewares/api/contracts/pim.yaml",
    "start": "NODE_ENV=production node build/server/index.js",
    "stylelint": "stylelint **/*.css",
    "tsc": "tsc"
  },
  "dependencies": {
    "@apollo/client": "3.3.13",
    "@graphql-mesh/config": "0.15.1",
    "@graphql-mesh/runtime": "0.13.1",
    "@loadable/component": "5.14.1",
    "@loadable/server": "5.14.2",
    "apollo-server-express": "2.21.2",
    "compression": "1.7.4",
    "core-js": "3.9.1",
    "cross-fetch": "3.1.2",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/node": "7.13.12",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/register": "7.13.8",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/schema-ast": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@graphql-mesh/cli": "0.27.2",
    "@graphql-mesh/openapi": "0.14.1",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/webpack-plugin": "5.14.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@stoplight/prism-cli": "4.1.2",
